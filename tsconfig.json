{
  "compilerOptions": {
    "target": "ES2015",
    "module": "commonjs",
    "jsx": "react",
    "outDir": "dist",
    "strict": true,
    "strictNullChecks": true,
    "baseUrl": "src/public",
    "paths": {
      "@components/*": [
        "components/*"
      ],
      "@models/*": [
        "models/*"
      ],
      "@services/*": [
        "services/*"
      ]
    }
  }
}